{
    "extends": "./.svelte-kit/tsconfig.json",
    "compilerOptions": {
        "paths": {
            "$lib": ["src/lib"],
            "$lib/*": ["src/lib/*"],
            "$components/*": ["src/components/*"],
            "$templates/*": ["src/templates/*"],
            "$layouts/*": ["src/layouts/*"],
            "$utils/*": ["src/utils/*"],
            "$icons/*": ["node_modules/svelte-material-icons/*"],
            "$stores/*": ["src/stores/*"],
            "$main/*": ["src/main/*"],
            "$types/*": ["src/types/*"],
        }
    },
    "include": ["src/**/*.d.ts", "src/**/*.js", "src/**/*.ts", "src/**/*.svelte", "src/hooks.=jts"]
}
